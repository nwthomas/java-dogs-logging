<?xml version = "1.0" encoding = "UTF-8"?>

<!--*** Part 1-->

<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%d{yyyy-MM-dd' 'HH:mm:ss.SSS}] [%25.25class] [%thread] [%line] [%-5level] %m%n</pattern>
        </encoder>
    </appender>
</configuration>